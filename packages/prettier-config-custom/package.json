{
  "name": "prettier-config-custom",
  "version": "0.0.0",
  "main": ".prettier.config.ts",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,css,less,scss,html,md,json,yaml,eslintrc}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,css,less,scss,html,md,json,yaml,eslintrc}\"",
    "license-checker": "license-checker --onlyAllow 'MIT;BSD;WTFPL;Apache-2.0;Apache*;BSD-2-Clause;ISC;BSD-3-Clause;BSD*;CC0-1.0;CC-BY-3.0;Unlicense;UNLICENSED;CC-BY-4;Python-2.0;MPL-2.0'"
  },
  "dependencies": {
    "prettier": "latest"
  },
  "devDependencies": {
    "license-checker": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
