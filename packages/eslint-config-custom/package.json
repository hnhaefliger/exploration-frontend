{
  "name": "eslint-config-custom",
  "version": "0.0.0",
  "main": ".eslintrc",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,css,less,scss,html,md,json,yaml,eslintrc}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,css,less,scss,html,md,json,yaml,eslintrc}\"",
    "license-checker": "license-checker --onlyAllow 'MIT;BSD;WTFPL;Apache-2.0;Apache*;BSD-2-Clause;ISC;BSD-3-Clause;BSD*;CC0-1.0;CC-BY-3.0;Unlicense;UNLICENSED;CC-BY-4;Python-2.0;MPL-2.0'"
  },
  "dependencies": {
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "7.31.8",
    "eslint-config-turbo": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest"
  },
  "devDependencies": {
    "prettier": "latest",
    "prettier-config-custom": "workspace:*",
    "license-checker": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
